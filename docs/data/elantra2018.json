\u1ee5p trong nh\u00e0 t\u1eafm v\u00ec trong ph\u00f2ng tr\u1ecd kh\u00f4ng c\u00f3 g\u01b0\u01a1ng l\u1edbn <img src=\"data:image\/gif;base64,R0lGODlhAQABAIAAAAAAAP\/\/\/yH5BAEAAAAALAAAAAABAAEAAAIBRAA7\" class=\"smilie smilie--sprite smilie--sprite1\" alt=\":)\" title=\"Smile :)\" loading=\"lazy\" data-shortname=\":)\"\/> <br\/> <script class=\"js-extraPhrases\" type=\"application\/json\"> \/\/<![CDATA[ { \"lightbox_close\": \"Close\", \"lightbox_next\": \"Next\", \"lightbox_previous\": \"Previous\", \"lightbox_error\": \"The requested content cannot be loaded. Please try again later.\", \"lightbox_start_slideshow\": \"Start slideshow\", \"lightbox_stop_slideshow\": \"Stop slideshow\", \"lightbox_full_screen\": \"Full screen\", \"lightbox_thumbnails\": \"Thumbnails\", \"lightbox_download\": \"Download\", \"lightbox_share\": \"Share\", \"lightbox_zoom\": \"Zoom\", \"lightbox_new_window\": \"New window\", \"lightbox_toggle_sidebar\": \"Toggle sidebar\" } \/\/]]> <\/script> <div class=\"bbImageWrapper js-lbImage\" title=\"image-e3887d2b30255b10cd9e62e000071483d7d293f39e66c3d4bf29840b611d42fa-V.jpg\" data-src=\"https:\/\/greenupload.com\/images\/2019\/06\/25\/image-e3887d2b30255b10cd9e62e000071483d7d293f39e66c3d4bf29840b611d42fa-V.jpg\" data-lb-sidebar-href=\"\" data-lb-caption-extra-html=\"\" data-single-image=\"1\"> <img src=\"https:\/\/greenupload.com\/images\/2019\/06\/25\/image-e3887d2b30255b10cd9e62e000071483d7d293f39e66c3d4bf29840b611d42fa-V.jpg\" data-url=\"https:\/\/greenupload.com\/images\/2019\/06\/25\/image-e3887d2b30255b10cd9e62e000071483d7d293f39e66c3d4bf29840b611d42fa-V.jpg\" class=\"bbImage\" data-zoom-target=\"1\" style=\"\" alt=\"image-e3887d2b30255b10cd9e62e000071483d7d293f39e66c3d4bf29840b611d42fa-V.jpg\" title=\"\" width=\"\" height=\"\" loading=\"lazy\"\/> <\/div> <br\/> <div class=\"bbImageWrapper js-lbImage\" title=\"image-c6ba5df8385a50fef613c237c9c2aa30140c1b43503d18361fed1a282ac864f8-V.jpg\" data-src=\"https:\/\/greenupload.com\/images\/2019